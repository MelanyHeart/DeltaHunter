on disconnect:
	loop {i::genericPlayerStats::*}:
		if {p::false::%player%::%loop-value%} is 0:
			clear {p::false::%player%::%loop-value%}
	clear {p::false::%player%::*}

	loop {i::genericPlayerMultipliers::*}:
		if {p::false::%player%::%loop-value%} is 1:
			clear {p::false::%player%::%loop-value%}
	clear {p::false::%player%::*}